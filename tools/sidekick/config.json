{
  "searchApiEndpoints": {
    "ingest": "https://search.netcentric.biz/ingest?path={{path}}",
    "delete": "https://search.netcentric.biz/delete?path={{path}}"
  },
  "plugins": [
    {
      "id": "cn-search-update",
